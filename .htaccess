# INICIO Cache-Control Headers
<ifModule mod_headers.c>
	<filesMatch "\.(css)$">
	Header set Cache-Control "private, no-cache"
	</filesMatch>
	<FilesMatch "\.(js)$">
	Header set Cache-Control "private, no-cache"
	</FilesMatch>
	<FilesMatch ".(woff2|woff|ttf|otf|eot)$">
	Header set Cache-Control "max-age=31536000, private"
	</FilesMatch>
	<filesMatch "\.(x?html?|php)$">
	Header set Cache-Control "private, no-cache"
	</filesMatch>
</ifModule>
